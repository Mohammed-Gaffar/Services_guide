<body dir="rtl">

    <nav class="main_navbar">
        <nav class="navbar navbar-expand-lg top_nav">
            <div class="container-fluid">
                <a class="kfu_logo_wrap" asp-area="" asp-controller="Services" asp-action="Index">
                    <img src="~/assets/imgs/KFU_Logo_Dark.png" alt="KFU ICON LOGO">
                </a>

                <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#navbarSupportedContent"
                        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="bi bi-list"></i>
                </button>
                <div class="offcanvas offcanvas-end" id="navbarSupportedContent">
                    <div class="offcanvas-header">
                        <h5 class="offcanvas-title" id="offcanvasNavbarLabel">Services Catalog</h5>
                        <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
                    </div>
                    <div class="offcanvas-body justify-content-end">
                        <ul class="navbar-nav mb-2 mb-lg-0">
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Services" asp-action="Index">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Users" asp-action="Index">users</a>
                            </li>
                        </ul>
                        <ul class="navbar-nav align-items-center d-none d-lg-flex">
                            <li class="nav-item">|</li>
                        </ul>
                        <ul class="navbar-nav">
                                <a class="nav-link" asp-area="" asp-controller="Services" asp-action="Index">Logout</a>
                            </li>
                        </ul>
                       
                    </div>
                </div>
            </div>
        </nav>
    </nav>


    <main role="main">
        @RenderBody()
    </main>

    <footer class="bg-blue py-3">
        <p class="m-0 text-white text-center">
            All Rights Reserved for KFU © 2024 | Designed and Developed by Deanship of E-Learning and Information Technology
        </p>
    </footer>


    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/jquery-3.4.1.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>

    <script src="~/lib/sweetalert2/sweetalert2.min.js" asp-append-version="true"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>

    @* <script src="//cdn.datatables.net/2.0.3/js/dataTables.min.js" asp-append-version="true"></script> *@


    @await RenderSectionAsync("Scripts", required: false)
